<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            style="-fx-background-color: ADD8E6;" xmlns="http://javafx.com/javafx/8" fx:controller="ChangePassword">
    <children>
        <VBox layoutX="24.0" layoutY="90.0">
            <children>
                <HBox layoutX="24.0" layoutY="68.0">
                    <children>
                        <Label alignment="CENTER" layoutX="70.0" layoutY="150.0" prefHeight="43.0" prefWidth="97.0"
                               text="Username:-" textAlignment="CENTER">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets right="18.0"/>
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="usernametextfield" layoutX="183.0" layoutY="156.0" prefHeight="31.0"
                                   prefWidth="307.0" promptText="Username">
                            <cursor>
                                <Cursor fx:constant="TEXT"/>
                            </cursor>
                            <padding>
                                <Insets left="10.0" right="10.0"/>
                            </padding>
                        </TextField>
                    </children>
                </HBox>
                <HBox layoutX="24.0" layoutY="111.0">
                    <children>
                        <Label layoutX="70.0" layoutY="202.0" prefHeight="43.0" prefWidth="133.0" text="Old Password:-"
                               textAlignment="CENTER">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <PasswordField fx:id="oldPasswordField" layoutX="183.0" layoutY="208.0" prefHeight="31.0"
                                       prefWidth="307.0" promptText="Old Password">
                            <cursor>
                                <Cursor fx:constant="TEXT"/>
                            </cursor>
                        </PasswordField>
                    </children>
                </HBox>
                <HBox layoutX="24.0" layoutY="154.0">
                    <children>
                        <Label layoutX="70.0" layoutY="202.0" prefHeight="43.0" prefWidth="137.0" text="New Password:-"
                               textAlignment="CENTER">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <PasswordField fx:id="newpasswordfield" layoutX="183.0" layoutY="208.0" prefHeight="31.0"
                                       prefWidth="307.0" promptText="New Password">
                            <cursor>
                                <Cursor fx:constant="TEXT"/>
                            </cursor>
                        </PasswordField>
                    </children>
                </HBox>
                <HBox layoutX="24.0" layoutY="197.0">
                    <children>
                        <Label layoutX="70.0" layoutY="202.0" prefHeight="43.0" prefWidth="166.0"
                               text="Confirm Password:-" textAlignment="CENTER">
                            <font>
                                <Font size="18.0"/>
                            </font>
                            <HBox.margin>
                                <Insets right="5.0"/>
                            </HBox.margin>
                        </Label>
                        <PasswordField fx:id="confirmpasswordfield" layoutX="183.0" layoutY="208.0" prefHeight="31.0"
                                       prefWidth="307.0" promptText="Confirm Password">
                            <cursor>
                                <Cursor fx:constant="TEXT"/>
                            </cursor>
                        </PasswordField>
                    </children>
                </HBox>
            </children>
        </VBox>
        <Button layoutX="227.0" layoutY="338.0" mnemonicParsing="false" onAction="#onChangeClick" prefHeight="31.0"
                prefWidth="137.0" text="Reset Password"/>
        <Button layoutX="24.0" layoutY="338.0" mnemonicParsing="false" onAction="#onBackButton" text="Back"/>
        <Label fx:id="messegelabel" alignment="CENTER" layoutX="23.0" layoutY="240.0" prefHeight="43.0"
               prefWidth="545.0" textAlignment="CENTER" textFill="#fc0f0f">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label alignment="CENTER" layoutX="196.0" layoutY="14.0" prefHeight="43.0" prefWidth="199.0"
               text="Change Password Page" textAlignment="CENTER">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
