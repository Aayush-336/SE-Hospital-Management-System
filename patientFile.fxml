<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="550.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PatientController">
    <children>
        <VBox layoutY="100.0" maxWidth="-Infinity" prefWidth="800.0" spacing="20.0">
            <children>
                <HBox layoutY="200.0">
                    <children>
                        <Pane fx:id="addpatientPane" layoutX="14.0" layoutY="200.0" onMouseClicked="#openAddPatient"
                              prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@Resources/add_patient.png"/>
                                    </image>
                                </ImageView>
                                <Text layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="imageDesc"
                                      text="Add Patient"/>
                            </children>
                        </Pane>
                        <Pane fx:id="patientPane1" layoutX="300.0" layoutY="200.0" onMouseClicked="#dischargePatient"
                              prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@Resources/discharge_patient.png"/>
                                    </image>
                                </ImageView>
                                <Text layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="imageDesc"
                                      text="Discharge Patient"/>
                            </children>
                        </Pane>
                        <Pane fx:id="patientPane111" onMouseClicked="#admitPatient" prefHeight="200.0"
                              prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@Resources/admit_patient.png"/>
                                    </image>
                                </ImageView>
                                <Text layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="imageDesc"
                                      text="Admit Patient"/>
                            </children>
                        </Pane>
                        <Pane fx:id="patientPane11" onMouseClicked="#searchPatient" prefHeight="200.0"
                              prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@Resources/search_patient.png"/>
                                    </image>
                                </ImageView>
                                <Text layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="imageDesc"
                                      text="Search Patient"/>
                            </children>
                        </Pane>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="20.0"/>
                    </padding>

                </HBox>
                <HBox layoutX="10.0" layoutY="210.0" prefWidth="800.0">
                    <children>
                        <Pane fx:id="addServicePane" layoutX="14.0" layoutY="200.0" onMouseClicked="#addService"
                              prefHeight="200.0" prefWidth="200.0">
                            <children>
                                <ImageView fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@Resources/add_service.png"/>
                                    </image>
                                </ImageView>
                                <Text layoutY="181.0" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="imageDesc"
                                      text="Add Service"/>
                            </children>
                        </Pane>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets left="20.0"/>
                    </padding>
                </HBox>
            </children>
        </VBox>

    </children>
</AnchorPane>
